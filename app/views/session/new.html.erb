<img src=<%= image_path("cover.jpg") %> class="full-background-image">

<div class="new-session-form-section">
  <%= form_tag session_index_path, method: :post, html: { class: "user-form new-session-form" } do %>
    <h1 class="session-form-title">Welcome back!</h1>
    <%= email_field_tag :email, nil, placeholder: "email@domain.com", required: true, autocomplete: false %><br>
    <%= password_field_tag :password, nil, placeholder: "supersecret123", required: true %><br>
    <%= submit_tag "Log In" %>
  <% end %>
</div>